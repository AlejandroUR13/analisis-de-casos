def burbujaP(arr):
    n = len(arr)
    for i in range(n):
        for j in range(0, n - i - 1):
            if arr[j] > arr[j + 1]:
                arr[j], arr[j + 1] = arr[j + 1], arr[j]

# Ejemplo de uso
lista = [5,2,47,9,24,14]

print("Lista original:", lista)
burbujaP(lista)
print("Lista ordenada:", lista)
